        <Button Content="Terug" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Width="120" Height="29" Click="Button_Click"/>
        <Label Content="D&amp;M Weed Shop" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontFamily="Sitka Heading" FontSize="20"/>
        <Button x:Name="registreer_button" Content="Registreer" HorizontalAlignment="Left" Margin="10,116,0,0" VerticalAlignment="Top" Width="120" Height="29" Click="registreer_button_Click"/>

    </Grid>
</Window>
