        xmlns:local="clr-namespace:StoreServiceWpfClient"
        mc:Ignorable="d"
        Title="Register" Height="280.598" Width="263.301">
    <Grid Margin="2,0,0,0.333" HorizontalAlignment="Left" Width="311">
